#ess_loader p {position:absolute;top:33%;width:40px;height:40px;left:49%;background-position:0 0;background-color:transparent;background-repeat:no-repeat;opacity:1}
#ess_loader div {position:absolute;top:45%;left:45%;width:auto;padding:2px 20px;height:30px;border-radius:6px;background:#F60;color:#FFF;font-size:13px;font-weight:bold;line-height:30px;text-align:center;vertical-align:middle;opacity:1;}
.wrap div.updated, .wrap div.error, .media-upload-form div.error {margin:5px 0 7px !important;}


/* Form Add Feed */
.widefat td {vertical-align: middle;}
#btAddESS {font-size:18px;cursor:pointer;}
#btViewErrors {background-color:#FFEBE8;color:#C00;cursor:pointer;padding:6px 15px;}
#add_feed_form {padding:10px;margin:0;display:none;border-bottom:1px solid #CCC;border-left:1px solid #CCC;border-right:1px solid #CCC;}
	#add_feed_form #titlediv {box-shadow:none;}
	#add_feed_form #titlediv #title {width:75%;font-size:1.4em;height:32px;}
	#add_feed_form i {color:grey;font-weight:normal;}


/* Import ESS */
.arrow_cont {width:35px;height:30px!important;position:relative;top:1px;}
.arrow_box {position:relative;background-color:#EAEAEA;cursor:pointer;top:10px;} 
.arrow_box:after {top:100%;border: solid transparent;content:" ";height:0;width:0;position:absolute;pointer-events:none;cursor:pointer;} 
.arrow_box:after {border-color:rgba(33,117,155,0);border-top-color:#EAEAEA;border-width:8px;left:20%;margin-left:-4px;cursor:pointer;}
.reload_box {cursor:pointer;width:40px;background-repeat:no-repeat;background-position:7px 2px;height:30px !important;}
#bt_add_feed {height:28px;}

/* Export ESS */
#poststuff input[type='text'] {padding:6px 8px;width:100%;}
	#poststuff .form-table em {float:left;clear:right;color:#888;line-height:30px;}
	#poststuff .form-table td {padding:3px 10px;}
	#poststuff .form-table th {font-size: 1em;margin:1.33em 0;}
	#poststuff .form-table .iphone {float:left;clear:right;margin-right:10px;}
	#poststuff .submit {border-top:1px dotted #CCC;margin-bottom:0;padding-bottom:6px;}
	/* Explain block */
	.ess_explain {border-radius:3px;border-style:solid;border-width:1px;margin:5px 0 15px;background-color:#f2f7fa;border-color:#9dc7d5;opacity:0.8;}
	.ess_explain p {margin:0.5em 0;color:#555;font-style:normal;font-weight:normal;}
	.ess_explain img {float:left;clear:both;margin:2px 6px;}
	/* social icon */
	.span-social-icon {display:inline-block;height:16px;overflow:hidden;text-indent:16px;vertical-align:top;white-space:nowrap;width:16px;}
	.input-social-icon {padding:6px 0 6px 25px !important;background-repeat:no-repeat;background-position:4px 4px;}
