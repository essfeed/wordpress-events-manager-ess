	/* TimeZone Picker */
	#timezone-picker {border-radius:6px 6px 0 0;border:1px solid #ccc;width:400px;height:200px;overflow:hidden;border-bottom:none;}
	#timezone-detect {text-align:center;width:400px;padding:6px 0;height:auto;}
	#timezone-fieldset {position:relative;}
		#current-timezone {position:absolute;top:-40px;width:400px;height:20px;text-align:center;vertical-align:middle;}
	
	/* JQuery Plugin: jquery.toggles.min.js */
	#ess_mode {display:inline-block;vertical-align:middle;width:150px;}
	.iphone {height:30px;margin:3px auto;width:75px;}
	.slide {overflow:hidden;cursor:pointer;}
	.slide .on,.slide .off,.slide .blob {float:left;}
	.slide .blob {position: relative;z-index:99;cursor:hand;cursor:grab;cursor:-moz-grab;cursor:-webkit-grab;}
	.iphone .slide {border-radius: 3px;}
	.iphone .slide .on,.iphone .slide .off,.iphone .slide .blob {color:white;font-size:13px;font-weight: bold;text-shadow: 0 0 8px rgba(0,0,0,0.5);}
	.iphone .slide .on {border-radius:3px 0 0 3px;line-height:27px;background-color: #21759B;background-image:linear-gradient(to bottom, #2A95C5, #21759B);border-color: #21759B #21759B #1E6A8D;color:#FFFFFF;text-decoration: none;text-shadow: 0 1px 0 rgba(0, 0, 0, 0.1);}
	.iphone .slide .on:hover {background-color:#278AB7;background-image:linear-gradient(to bottom,#2E9FD2,#21759B);border-color:#1B607F;box-shadow:0 1px 0 rgba(120, 200, 230, 0.6) inset;color:#FFFFFF;text-shadow:0 -1px 0 rgba(0,0,0,0.3);}
	.iphone .slide .off {border-radius:0 3px 3px 0;color:rgb(130,130,130);text-shadow: 0 0 1px white;line-height: 27px;border:1px solid #CCC;box-shadow: 0 1px 0 #FFF inset;}
	.iphone .slide .on:after {height: 50%;content:'';margin-top:-19%;display: block;border-radius:3px;margin-left: 10%;}
	.iphone .slide .off {background:#ECECEC;}
	.iphone .slide .off:after {height:50%;content:'';margin-top:-19%;display:block;margin-right:10%;border-radius:3px;}
	.iphone .slide .blob {border-radius:0px;background:#d1d1d1;border:1px solid #bbb;box-shadow:0 1px 0 #EAEAEA inset;height:28px !important;width: 28px !important;}